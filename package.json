{
  "name": "redux-little-router",
  "version": "4.0.0",
  "description": "A barebones routing solution for Redux applications.",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "cd lib || npm run build-lib",
    "preversion": "npm run check",
    "version": "npm run clean && npm run build && git add -A dist",
    "clean-dist": "rimraf dist",
    "build-dist-min": "webpack --bail --config config/webpack/webpack.config.js",
    "build-dist-dev": "webpack --bail --config config/webpack/webpack.config.dev.js",
    "build-dist": "npm run clean-dist && npm run build-dist-min && npm run build-dist-dev",
    "clean-lib": "rimraf lib",
    "build-lib": "npm run clean-lib && babel src -d lib --copy-files",
    "watch-lib": "watch 'npm run build-lib' src/ -d",
    "clean": "npm run clean-lib && npm run clean-dist",
    "build": "npm run build-lib && npm run build-dist",
    "lint-src": "eslint --color --ext .js,.jsx -c config/eslint/.eslintrc src demo/*.jsx",
    "lint-test": "eslint --color --ext .js,.jsx -c config/eslint/.eslintrc-test src test",
    "lint": "concurrently 'npm run lint-src' 'npm run lint-test'",
    "test": "karma start config/karma/karma.conf.js",
    "test-ci": "karma start --browsers PhantomJS,Firefox config/karma/karma.conf.coverage.js",
    "test-cov": "karma start config/karma/karma.conf.coverage.js",
    "test-dev": "karma start config/karma/karma.conf.dev.js",
    "check": "npm run lint && npm run test",
    "check-ci": "npm run lint && npm run test-ci",
    "check-cov": "npm run lint && npm run test-cov",
    "check-dev": "npm run lint && npm run test-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tptee@github.com/FormidableLabs/redux-little-router.git"
  },
  "keywords": [
    "redux",
    "router",
    "middleware"
  ],
  "author": "Tyler Thompson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/redux-little-router/issues"
  },
  "homepage": "https://github.com/FormidableLabs/redux-little-router#readme",
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "coveralls": "^2.11.11",
    "exenv": "^1.2.1",
    "history": "^3.0.0",
    "lodash.find": "^4.4.0",
    "rimraf": "^2.5.3",
    "url-pattern": "^1.0.1",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.0.1",
    "eslint-config-defaults": "^10.0.0-alpha.1",
    "eslint-plugin-filenames": "^1.0.0",
    "eslint-plugin-react": "^5.2.2",
    "isparta-loader": "^2.0.0",
    "karma": "^1.1.1",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "opener": "^1.4.1",
    "phantomjs-prebuilt": "^2.1.7",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "watch": "^0.19.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "redux": "^3.0.0",
    "react": "^0.14.0 || ^15.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0"
  }
}
